{{>header}}
{{#each news}}
<div class="card card-body">
    <div class="media align-items-center align-items-lg-start text-center text-lg-left flex-column flex-lg-row">
        <div class="mr-5 mb-3 mb-lg-0"> <img src={{this.urlToImage}} width="230" height="150" alt=""> </div>
        <div class="media-body">
            <h6 class="media-title font-weight-semibold"> <a href="{{this.url}}" data-abc="true">{{this.title}}</a>
            </h6>
            <p class="mb-3">
                {{this.description}}
            </p>
        </div>
        <div class="mt-4 mt-lg-0 ml-lg-3 text-center">
            <a href={{this.url}} class="btn btn-primary mt-4 text-white">اقرا الخبر</a>
        </div>
    </div>
</div>


{{/each}}
{{>footer}}